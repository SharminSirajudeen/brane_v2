{
  "name": "brane",
  "version": "0.1.0",
  "description": "Modular AI agent creation platform with local-first architecture",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:electron": "electron .",
    "dev:react": "vite",
    "dev:cli": "tsx src/cli/index.ts",
    "build": "npm-run-all build:*",
    "build:react": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "start": "electron .",
    "cli": "node dist/cli/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,json,md}"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "electron",
    "neuron",
    "rag"
  ],
  "author": "Sharmin Sirajudeen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "electron": "^29.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chokidar": "^3.6.0",
    "faiss-node": "^0.5.1",
    "node-llama-cpp": "^2.8.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.1"
  }
}